#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/panshihuang/Desktop/gitRepositories/mini-vue-learn/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/panshihuang/Desktop/gitRepositories/mini-vue-learn/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+rollup@3.21.6/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+rollup@3.21.6/node_modules/rollup/dist/bin/rollup" "$@"
fi
