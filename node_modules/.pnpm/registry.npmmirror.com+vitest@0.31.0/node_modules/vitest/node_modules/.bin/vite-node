#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/panshihuang/Desktop/gitRepositories/mini-vue-learn/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/panshihuang/Desktop/gitRepositories/mini-vue-learn/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+vite-node@0.31.0_@types+node@20.1.3/node_modules/vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+vite-node@0.31.0_@types+node@20.1.3/node_modules/vite-node/vite-node.mjs" "$@"
fi
